{
	"env": {
			"browser": true,
			"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"@rocketseat/eslint-config/react"
	],
	"overrides": [
	],
	"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module",
			"project": [
				"./tsconfig.json"
			]
	},
	"plugins": [
			"react"
	],
	"rules": {
		"react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }]
	}
}
